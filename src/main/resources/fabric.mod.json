{
	"schemaVersion": 1,
	"id": "${id}",
	"version": "${version}",
	"name": "${name}",
	"description": "${description}",
	"authors": [
		"Scubakay"
	],
	"contact": {
		"homepage": "${homepage}",
		"sources": "${repository}",
		"issues": "${repository}/issues"
	},
	"custom": {
		"modmenu": {
			"links": {
				"modmenu.discord": "${discord}"
			}
		}
	},
	"license": "LGPL-3.0",
	"environment": "*",
	"entrypoints": {
		"main": [
			"net.darktree.fungible.Fungible"
		],
		"client": [
			"net.darktree.fungible.client.FungibleClient"
		]
	},
	"mixins": [
		"fungible.mixins.json",
		{
			"config": "fungible.client.mixins.json",
			"environment": "client"
		}
	],
	"depends": {
		"fabricloader": ">=0.15",
		"minecraft": "${mcdep}"
	}
}
